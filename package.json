{
  "name": "test-api",
  "author": "Leonardo",
  "license": "Strictly Private",
  "version": "1.15.1",
  "description": "Test",
  "main": "src/server.ts",
  "engines": {
    "node": ">=16.11",
    "npm": ">=7.7"
  },
  "scripts": {
    "copy": "cp ./config/static/eos/nftProperties.json ./src/utils/static/nftProperties.json && cp ./config/static/eos/leaderboard.json ./src/utils/static/leaderboard.json",
    "predev": "npm run copy",
    "pretest": "npm run copy",
    "prettier": "prettier --config ./.prettierrc.json --loglevel warn --write '**/*.{ts,json}'",
    "tsoa": "node tsoa-routes-gen.js && tsoa spec-and-routes -c tsoa-gen.json",
    "build": "npm run tsoa && tsc --outDir build --experimentalDecorators",
    "start": "node build/src/server.js",
    "dev": "npm run tsoa && AWS_PROFILE=eosDev AWS_SDK_LOAD_CONFIG=1 nodemon",
    "repl": "ts-node  --require ./src/scripts/repl",
    "dev:hosted": "concurrently \"npm run ssh:mongo\" \"npm run ssh:redis\" \"npm run dev\" --kill-others",
    "ssh:mongo": "ssh -i ~/.ssh/id_rsa -L 27017:10.150.4.35:27017 ubuntu@3.143.142.127 -N",
    "ssh:redis": "ssh -i ~/.ssh/id_rsa -L 6379:10.150.4.219:6379 ubuntu@3.143.142.127 -N",
    "lint": "eslint . --ext .ts --cache",
    "lint:fix": "eslint . --ext .ts --cache --fix",
    "pretty": "prettier --config ./.prettierrc.json --check '**/*.{ts,json}'",
    "pretty:fix": "prettier --config ./.prettierrc.json --write '**/*.{ts,json}'",
    "format": "npm run lint:fix && npm run pretty:fix",
    "type-check": "tsc --pretty --noEmit",
    "test": "NODE_ENV=test.unit jest __tests__/__unit__/**/*.test.ts --runInBand",
    "test:dev": "NODE_ENV=test.dev jest __tests__/__integration__/*.test.ts",
    "test:local": "NODE_ENV=test.local jest __tests__/__integration__/*.test.ts",
    "test:qa": "NODE_ENV=test.qa jest __tests__/__integration__/*.test.ts",
    "test:all": "npm run type-check && npm run lint && npm run pretty && npm run test",
    "configureDb": "ts-node ./src/scripts/configureDb",
    "seedData": "ts-node ./src/scripts/seedData",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "@types/auth0": "^2.35.5",
    "@types/bad-words": "^3.0.1",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^1.4.7",
    "@types/reflect-metadata": "^0.1.0",
    "auth0": "^2.42.0",
    "aws-sdk": "^2.1226.0",
    "axios": "^0.27.2",
