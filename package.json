    "bcryptjs": "^2.4.3",
    "blob": "0.1.0",
    "body-parser": "^1.20.0",
    "bullmq": "^1.91.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.0",
    "debug": "^4.3.4",
    "dotenv": "^8.2.0",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.3",
    "helmet": "^5.1.1",
    "http-errors": "^1.8.0",
    "i18next": "^21.9.2",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-middleware": "^3.2.1",
    "ical-generator": "^3.5.1",
    "inquirer": "^8.2.2",
    "jest-mock-extended": "^2.0.7",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.2",
    "mongodb": "4.5.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "redlock": "^5.0.0-beta.2",
    "slugify": "^1.6.5",
    "sqs-consumer": "^5.6.0",
    "stripe": "^8.222.0",
    "swagger-ui-express": "^4.5.0",
    "ts-essentials": "^7.0.3",
    "tsoa": "^3.14.1",
